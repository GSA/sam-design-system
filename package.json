{
  "name": "sam-design-system",
  "version": "10.0.15",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "npm run test:components && npm run test:material-extensions && npm run test:sam-formly && npm run test:layouts",
    "test:components": "ng test components -- --no-watch --no-progress --code-coverage --browsers=ChromeHeadlessCI --source-map=false",
    "test:material-extensions": "ng test sam-material-extensions -- --no-watch --no-progress --code-coverage --browsers=ChromeHeadlessCI --source-map=false",
    "test:sam-formly": "ng test sam-formly -- --no-watch --no-progress --code-coverage --browsers=ChromeHeadlessCI --source-map=false",
    "test:layouts": "ng test layouts -- --no-watch --no-progress --code-coverage --browsers=ChromeHeadlessCI --source-map=false",
    "lint": "./node_modules/.bin/nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "federalist": "npm install --only=dev && npm run demo:docs && ng build --project=sam-design-system-site --prod --base-href=$BASEURL/ && npm run demo:stackblitzes-federalist && npm run build:docs",
    "version": "node ./tools/scripts/update-libs-version.js && git add .",
    "postversion": "git push && git push --tags",
    "changelog": "gren changelog --override && git add . && git commit -m \"Add changelog\" && git push",
    "release-notes": "gren release",
    "publish:libs": "node ./tools/scripts/pack-and-publish.js",
    "release:patch": "npm version patch && npm run release-notes && npm run changelog",
    "release:minor": "npm version minor && npm run release-notes && npm run changelog",
    "release:major": "npm version major && npm run release-notes && npm run changelog",
    "demo:docs:components": "npx compodoc -p libs/packages/components/tsconfig.lib.json -e json -d libs/documentation/src/lib/apidoc/components --minimal && node ./scripts/generate-docs.ts -f components",
    "demo:docs:formly": "npx compodoc -p libs/packages/sam-formly/tsconfig.lib.json -e json -d libs/documentation/src/lib/apidoc/formly --minimal  && node ./scripts/generate-docs.ts -f formly",
    "demo:docs:layouts": "npx compodoc -p libs/packages/layouts/tsconfig.lib.json -e json -d libs/documentation/src/lib/apidoc/layouts --minimal  && node ./scripts/generate-docs.ts -f layouts",
    "demo:docs": "npm run demo:docs:components && npm run demo:docs:formly && npm run demo:docs:layouts",
    "build:docs:components": "npx compodoc -p libs/packages/components/tsconfig.lib.json -n \"SAM.Gov Design System - Components\" --hideGenerator --disableCoverage -d dist/apps/sam-design-system-site/compodoc/components",
    "build:docs:formly": "npx compodoc -p libs/packages/sam-formly/tsconfig.lib.json -n \"SAM.Gov Design System - Formly\" --hideGenerator --disableCoverage -d dist/apps/sam-design-system-site/compodoc/formly",
    "build:docs:layouts": "npx compodoc -p libs/packages/layouts/tsconfig.lib.json -n \"SAM.Gov Design System - Layouts\" --hideGenerator --disableCoverage -d dist/apps/sam-design-system-site/compodoc/layouts",
    "build:docs": "npm run build:docs:components && npm run build:docs:formly && npm run build:docs:layouts",
    "compodocs": "npx compodoc -p libs/packages/components/tsconfig.lib.json -s --minimal",
    "nx": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "demo:stackblitzes": "ts-node --project misc/tsconfig.json misc/generate-stackblitzes.ts",
    "demo:stackblitzes-federalist": "ts-node --project misc/tsconfig.json misc/generate-stackblitzes.ts --federalist"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.1001.3",
    "@angular/animations": "^10.1.3",
    "@angular/cdk": "~10.2.1-sha-b23d130e1",
    "@angular/cli": "^10.1.3",
    "@angular/common": "^10.1.3",
    "@angular/compiler": "^10.1.3",
    "@angular/compiler-cli": "^10.1.3",
    "@angular/core": "^10.1.3",
    "@angular/forms": "^10.1.3",
    "@angular/material": "~10.2.1-sha-b23d130e1",
    "@angular/platform-browser": "^10.1.3",
    "@angular/platform-browser-dynamic": "^10.1.3",
    "@angular/router": "^10.1.3",
    "@compodoc/compodoc": "^1.1.11",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@gsa-sam/sam-styles": "0.0.105",
    "@ngx-formly/core": "^5.10.12",
    "@ngx-formly/schematics": "^5.10.12",
    "@nrwl/angular": "^10.4.10",
    "@nrwl/nx": "7.5.0",
    "@types/qs": "^6.9.5",
    "accessible-html5-video-player": "^1.0.6",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.11",
    "fs-extra": "^9.1.0",
    "highlight.js": "~9.18.1",
    "lodash-es": "^4.17.15",
    "minimist": "^1.2.5",
    "ngx-bootstrap-icons": "1.2.1",
    "ngx-highlightjs": "^4.1.1",
    "ngx-markdown": "^10.1.1",
    "ngx-toastr": "^13.2.0",
    "qs": "^6.9.6",
    "raw-loader": "1.0.0",
    "rxjs": "~6.5.5",
    "tsickle": "0.39.1",
    "tslib": "^2.0.1",
    "typescript": "4.0.3",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-ng-packagr": "~0.1000.8",
    "@angular/language-service": "^10.1.3",
    "@nrwl/workspace": "^10.4.10",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.0.0",
    "@types/node": "^14.14.22",
    "codelyzer": "^6.0.0",
    "css-loader": "^3.2.0",
    "dotenv": "6.2.0",
    "ejs": "3.1.5",
    "eslint": "6.8.0",
    "github-release-notes": "^0.17.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^10.0.0",
    "prettier": "2.0.4",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0"
  }
}
