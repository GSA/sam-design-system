<html lang="en">

<head>
  <style>
    html {
      font-family: -apple-system, system-ui, BlinkMacSystemFont, "Segoe UI", Roboto
    }

    html,
    body {
      width: 100vw;
      height: 100vh;
      display: flex;
      background-color: rgb(54, 50, 119);
      align-items: center;
      justify-content: center;
      color: #fafafa;
    }

    .loading {
      text-align: center;
    }
  </style>

</head>

<body>
  <div class="loading">
    <div>
      <img
        src="data:image/png;base64," />
    </div>
    <h3>Loading &laquo; <%=componentName%> - <%=demoName%> &raquo; demo</h3>
  </div>
  <form id="mainForm" method="post" action="<%= stackblitzUrl %>?file=<%=openFile%>" target="_self">
    <input type="hidden" name="project[template]" value="angular-cli">
    <input type="hidden" name="project[title]" value="<%= title %>">
    <input type="hidden" name="project[description]"
      value="Example usage of the <%= componentName %> widget from https://ng-bootstrap.github.io">
    <input type="hidden" name="project[dependencies]" value="<%= dependencies %>">
    <% for (const tag of tags) { %>
    <input type="hidden" name="project[tags][]" value="<%=tag%>">
    <% } %>
    <% for (const file of files) { %>
    <input type="hidden" name="project[files][<%= file.name%>]" value="<%= file.source %>">
    <% } %>
  </form>
  <script>document.getElementById("mainForm").submit();</script>
</body>

</html>
